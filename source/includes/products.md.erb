    "name": "Moisturizer"
  },
  "tags": [{
      "id": "prdtag_phdfnbtamnkypp8",
      "name": "best-sellers"
    },
    {
      "id": "prdtag_4rp64333hegzv1s",
      "name": "essentials"
    }
  ],
  "image_media": {
    "id": "file_b027hj4e3483rhi",
    "file_url": "https://storage.googleapis.com/onvoard/upload/product/image-media/e4c8b25345sdf4s2836tg5.jpg",
